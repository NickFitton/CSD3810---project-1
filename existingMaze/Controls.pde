void moveRight() {
  player.position.x = player.position.x + player.speed;
}
void moveDown() {
  player.position.y = player.position.y + player.speed;
}
void moveUp() {
  player.position.y = player.position.y - player.speed;
}
void moveLeft() {
  player.position.x = player.position.x - player.speed;
}
